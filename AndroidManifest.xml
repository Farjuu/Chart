<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chart"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <activity
            android:name=".Song"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.chart.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Option_select_activity"
            android:label="@string/title_activity_option_select_activity" >
        </activity>
        <activity
            android:name=".Input_Activity"
            android:label="@string/title_activity_input_" >
        </activity>
        <activity
            android:name=".Simple_View"
            android:label="@string/title_activity_simple__view" >
        </activity>
        <activity
            android:name=".Vi"
            android:label="@string/title_activity_vi" >
        </activity>
        <activity
            android:name=".Pie_activity"
            android:label="@string/title_activity_pie_activity" >
        </activity>
        <activity
            android:name=".Option_pane"
            android:label="@string/title_activity_option_pane" >
        </activity>
        <activity
            android:name=".Option"
            android:label="@string/title_activity_option" >
        </activity>
        <activity
            android:name=".BarActivity"
            android:label="@string/title_activity_bar" >
        </activity>
        <activity
            android:name=".Input_Bar"
            android:label="@string/title_activity_input__bar" >
        </activity>
        <activity
            android:name=".LineActivity"
            android:label="@string/title_activity_line" >
        </activity>
        <activity
            android:name=".Line"
            android:label="@string/title_activity_line" >
        </activity>
        <activity
            android:name=".SecondActivity"
            android:label="@string/title_activity_second" >
        </activity>
    </application>

</manifest>
